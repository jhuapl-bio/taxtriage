{
    "name": "ONT (Oxford Nanopore)",
    "description": "Optimized for long-read Oxford Nanopore sequencing",
    "preset": "ONT",
    "quality_threshold": 10,
    "min_read_length": 500,
    "enable_subsampling": false,
    "subsample_size": 50000,
    "max_cpus": 8,
    "max_memory": "32.GB",
    "kraken_db": "standard",
    "bracken_db": "standard",
    "top_taxa": 20,
    "confidence_threshold": 0.05,
    "enable_krona": true,
    "enable_multiqc": true,
    "docker_profile": "docker",
    "specific_parameters": {
        "long_read_mode": true,
        "skip_trimming": true,
        "skip_quality_filter": false,
        "nanofilt_quality": 10,
        "nanofilt_length": 500,
        "nanofilt_maxlength": 50000
    }
}