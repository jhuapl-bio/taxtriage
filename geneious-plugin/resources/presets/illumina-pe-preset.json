{
    "name": "Illumina PE",
    "description": "Optimized for Illumina paired-end sequencing",
    "preset": "ILLUMINA_PE",
    "quality_threshold": 20,
    "min_read_length": 50,
    "enable_subsampling": false,
    "subsample_size": 100000,
    "max_cpus": 4,
    "max_memory": "16.GB",
    "kraken_db": "standard",
    "bracken_db": "standard",
    "top_taxa": 20,
    "confidence_threshold": 0.1,
    "enable_krona": true,
    "enable_multiqc": true,
    "docker_profile": "docker",
    "specific_parameters": {
        "long_read_mode": false,
        "skip_trimming": false,
        "skip_quality_filter": false,
        "fastp_qualified_quality_phred": 20,
        "fastp_unqualified_percent_limit": 10,
        "fastp_length_required": 50,
        "merge_pairs": false
    }
}